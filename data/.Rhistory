legend.position = 'right',
legend.direction = 'vertical') +
scale_x_discrete(breaks = labels.arr,
labels = labels.counts.arr) +
guides(fill = guide_legend(ncol = 2,title = "Substrate")) +
labs(caption = paste0("n = ",sum(counts.df$counts)))
print(plot)
dev.off()
}
}
